{
  "name": "benchwarmers-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/lib __tests__/components",
    "test:integration": "jest __tests__/integration --config jest.config.integration.js",
    "test:e2e": "playwright test",
    "test:api": "jest __tests__/api",
    "test:business": "jest __tests__/unit/matching-algorithm.test.ts __tests__/unit/payment-calculations.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api",
    "test:ci": "npm run test:coverage && npm run test:integration -- --ci --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:test": "tsx scripts/seed-test-data.ts",
    "test:db:setup": "./scripts/test-db-setup.sh",
    "test:db:teardown": "docker-compose -f docker-compose.test.yml down",
    "server:dev": "tsx --watch src/server/index.ts",
    "server:start": "tsx src/server/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "appwrite:setup": "tsx scripts/setup-appwrite-collections.ts",
    "appwrite:create": "tsx scripts/create-appwrite-collections.ts",
    "appwrite:create-all": "tsx scripts/create-all-collections.ts",
    "appwrite:create-remaining": "tsx scripts/create-remaining-collections.ts",
    "appwrite:create-minimal": "tsx scripts/create-minimal-collections.ts",
    "setup": "npm run docker:up && sleep 5 && npm run db:push && npm run db:generate"
  },
